<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <%= metamagic %>
  <title><%= @title.nil? ? "Fitnesspapa" : @title %> </title>

  <link rel="shortcut icon" href="/assets/favicon.ico" />
  <link rel="dns-prefetch" href="http://www.google-analytics.com">
  <link rel="dns-prefetch" href="http://d1djl7kr085bc1.cloudfront.net">

  <%= stylesheet_link_tag 'filters', media: 'all'%>
  <%= csrf_meta_tags %>
  <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
  <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
  <![endif]-->
  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAs4tO9vd5JNhXOX8h492hEwfstspyTQAw&libraries=places"></script>
</head>
<body>

 <%= render :partial => 'layouts/tagmanager' %>
 <%= render :partial => 'layouts/header' %>
 <%= render :partial => 'layouts/notices' %>
 <%= render :partial => 'layouts/search' %>
 <%= yield %>
 <%= render :partial => 'layouts/secure' %>
 <%= render :partial => 'layouts/footer' %>
 <%= render :partial => 'layouts/analytics' %>

 <%= javascript_include_tag 'filters'%>
 <%= render :partial => 'shared/zopim' %>
 <script>
    <% unless current_user.try(:passport) %>
      $('#package').modal();
    <% end %>
   
    setTimeout(runner(), 3000);

   function runner(){
       var searchContainer = React.render(
        React.createElement(SearchContainer, null),
        document.getElementById('search-container')
      )
   }
   
 </script>
</body>
</html>
