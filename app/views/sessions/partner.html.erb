<div class="container">
	<div class="container-main">
		<div class="sqr-box-title">
		<h2>Partner Sign-In</h2>
		<p class="bottom-title">New to <span>Fitness<b>Papa</b></span>? <a href="<%= registrations_partner_path %>">Register Your Business</a></p>
		</div>
		<div class="sqr-boxs-cat">
			<%= form_tag sessions_partner_auth_path, :class => 'sign' do %>
			  <div class="form-sec form-sign" style="width: 258px;margin-left: 49px;">
				
				<div class="" style="margin-top:20px">
				 <input id="email" type="email" value="" name="user[email]" placeholder = 'Email' style="padding:12px;margin-bottom:18px;width:87%">
				</div>
				
				<div class="">
				 <input id="password" type="password" name="user[password]" value="" style="padding:12px;width:87%" placeholder = 'Password'>
				</div>
				
				<div class="form-terms" style="text-align:center">
				 <input type="checkbox" name="remember" id="remember" class="css-checkbox">
				 <label for="remember" class="css-label"><p>Remember Me</p></label>
				</div>
			    
				<div class="form-submit">
				  <input type="submit" value="SIGN-IN" style="">
				  <a href="<%= new_password_reset_path(:partner => true) %>" style="">Forgot Password?</a>
				</div>
			  </div>
			<%end %>
		</div>
	</div>
</div>