<div class="filter-result">
<div class="list-view">
	<ul class="grid" style="width:100%">
	<%@centers.each do |s|  %>
		<li class="comp-box barber-saloon">
		 <div class="comp-pic">
			<a href="<%= center_about_path(s.center.friendly_id) %>"> <%= image_tag attachment_url(s.center.centerinfo, :image, :fill, 150, 130) %></a>
		 </div>
		 <div class="comp-info">
			<div class="comp-title">
			  <a href="#"><h4><%= s.name %></h4></a>
			</div>
		
			<div class="comp-rate">
			  <img src="/assets/star.jpg" alt=""><a href="#">312 Reviews</a>
			</div>
		 </div>
		</li>
	<%end %>
	</ul>
</div>
</div>