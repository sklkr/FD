var owl = $("#sd-calendar");
owl.owlCarousel({
      items : 7, //10 items above 1000px browser width
      pagination: false,
      rewindNav: false,
      navigation: true,
      navigationText: [
       "<i class='icon-chevron-left icon-white'><</i>",
       "<i class='icon-chevron-right icon-white'>></i>"
      ],
  });

var owldata = $("#sd-calendar").data('owlCarousel');
owldata.jumpTo(<%= @difference.to_i %>)
